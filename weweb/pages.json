{
  "pages": [
    {
      "path": "/login",
      "name": "Login",
      "isPublic": true,
      "redirectIfAuthenticated": "/dashboard",
      "layout": "auth",
      "components": [
        {
          "type": "container",
          "class": "auth-card",
          "children": [
            {
              "type": "image",
              "src": "/logo.svg",
              "alt": "Tax Document Portal",
              "class": "logo"
            },
            {
              "type": "heading",
              "level": 1,
              "text": "Welcome Back"
            },
            {
              "type": "text",
              "content": "Sign in to manage your tax documents"
            },
            {
              "type": "form",
              "id": "loginForm",
              "children": [
                {
                  "type": "input",
                  "name": "email",
                  "label": "Email",
                  "inputType": "email",
                  "placeholder": "you@example.com",
                  "required": true,
                  "validation": "email"
                },
                {
                  "type": "input",
                  "name": "password",
                  "label": "Password",
                  "inputType": "password",
                  "placeholder": "Enter your password",
                  "required": true,
                  "minLength": 8
                },
                {
                  "type": "button",
                  "text": "Sign In",
                  "variant": "primary",
                  "fullWidth": true,
                  "loading": "{{variables.isLoading}}",
                  "onClick": "workflow:login"
                }
              ]
            },
            {
              "type": "link",
              "text": "Forgot password?",
              "href": "/forgot-password",
              "class": "text-center mt-4"
            },
            {
              "type": "divider",
              "text": "or"
            },
            {
              "type": "link",
              "text": "Create an account",
              "href": "/register",
              "class": "text-center"
            }
          ]
        }
      ]
    },
    {
      "path": "/register",
      "name": "Register",
      "isPublic": true,
      "redirectIfAuthenticated": "/dashboard",
      "layout": "auth",
      "components": [
        {
          "type": "container",
          "class": "auth-card",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Create Account"
            },
            {
              "type": "text",
              "content": "Start managing your tax documents today"
            },
            {
              "type": "form",
              "id": "registerForm",
              "children": [
                {
                  "type": "input",
                  "name": "fullName",
                  "label": "Full Name",
                  "inputType": "text",
                  "placeholder": "John Doe",
                  "required": true
                },
                {
                  "type": "input",
                  "name": "email",
                  "label": "Email",
                  "inputType": "email",
                  "placeholder": "you@example.com",
                  "required": true,
                  "validation": "email"
                },
                {
                  "type": "input",
                  "name": "password",
                  "label": "Password",
                  "inputType": "password",
                  "placeholder": "At least 8 characters",
                  "required": true,
                  "minLength": 8
                },
                {
                  "type": "input",
                  "name": "confirmPassword",
                  "label": "Confirm Password",
                  "inputType": "password",
                  "placeholder": "Confirm your password",
                  "required": true,
                  "validation": "match:password"
                },
                {
                  "type": "button",
                  "text": "Create Account",
                  "variant": "primary",
                  "fullWidth": true,
                  "loading": "{{variables.isLoading}}",
                  "onClick": "workflow:register"
                }
              ]
            },
            {
              "type": "text",
              "content": "Already have an account?",
              "class": "text-center mt-4"
            },
            {
              "type": "link",
              "text": "Sign in",
              "href": "/login",
              "class": "text-center"
            }
          ]
        }
      ]
    },
    {
      "path": "/dashboard",
      "name": "Dashboard",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Dashboard"
            },
            {
              "type": "button",
              "text": "Upload Documents",
              "variant": "primary",
              "icon": "upload",
              "href": "/upload"
            }
          ]
        },
        {
          "type": "grid",
          "columns": 4,
          "gap": 24,
          "class": "stats-grid",
          "children": [
            {
              "type": "stat-card",
              "title": "Total Documents",
              "value": "{{collections.UserDocuments.length}}",
              "icon": "file-text",
              "color": "primary"
            },
            {
              "type": "stat-card",
              "title": "Completed",
              "value": "{{collections.UserDocuments.filter(d => d.status === 'completed').length}}",
              "icon": "check-circle",
              "color": "success"
            },
            {
              "type": "stat-card",
              "title": "Processing",
              "value": "{{collections.UserDocuments.filter(d => d.status === 'processing').length}}",
              "icon": "loader",
              "color": "warning"
            },
            {
              "type": "stat-card",
              "title": "Needs Review",
              "value": "{{collections.UserDocuments.filter(d => d.status === 'needs_review').length}}",
              "icon": "alert-circle",
              "color": "error"
            }
          ]
        },
        {
          "type": "grid",
          "columns": 2,
          "gap": 24,
          "children": [
            {
              "type": "card",
              "title": "Recent Documents",
              "children": [
                {
                  "type": "table",
                  "data": "{{collections.RecentDocuments}}",
                  "columns": [
                    {
                      "key": "filename",
                      "label": "Filename",
                      "render": "link:/documents/{{row.id}}"
                    },
                    {
                      "key": "type",
                      "label": "Type",
                      "render": "badge"
                    },
                    {
                      "key": "status",
                      "label": "Status",
                      "render": "status-badge"
                    },
                    {
                      "key": "created_at",
                      "label": "Date",
                      "render": "date:relative"
                    }
                  ],
                  "emptyText": "No documents yet. Upload your first document!",
                  "onRowClick": "navigate:/documents/{{row.id}}"
                }
              ],
              "footer": {
                "type": "link",
                "text": "View all documents",
                "href": "/documents"
              }
            },
            {
              "type": "card",
              "title": "Recent Activity",
              "children": [
                {
                  "type": "timeline",
                  "data": "{{collections.ProcessingLogs}}",
                  "itemTemplate": {
                    "icon": "{{item.status === 'completed' ? 'check' : item.status === 'failed' ? 'x' : 'clock'}}",
                    "color": "{{item.status === 'completed' ? 'success' : item.status === 'failed' ? 'error' : 'warning'}}",
                    "title": "{{item.action | capitalize}}",
                    "description": "{{item.resource_type}} - {{item.status}}",
                    "time": "{{item.created_at | relativeTime}}"
                  },
                  "emptyText": "No recent activity"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "path": "/upload",
      "name": "Upload Documents",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Upload Documents"
            }
          ]
        },
        {
          "type": "card",
          "children": [
            {
              "type": "dropzone",
              "id": "fileDropzone",
              "accept": ".pdf,.png,.jpg,.jpeg,.mp3,.wav,.m4a",
              "multiple": true,
              "maxSize": 10485760,
              "onDrop": "workflow:handleFileDrop",
              "children": [
                {
                  "type": "icon",
                  "name": "upload-cloud",
                  "size": 48,
                  "class": "text-gray-400"
                },
                {
                  "type": "text",
                  "content": "Drag and drop files here, or click to browse",
                  "class": "text-lg font-medium mt-4"
                },
                {
                  "type": "text",
                  "content": "Supported: PDF, PNG, JPG, MP3, WAV (max 10MB)",
                  "class": "text-sm text-gray-500 mt-2"
                }
              ]
            },
            {
              "type": "conditional",
              "condition": "{{variables.uploadFiles.length > 0}}",
              "children": [
                {
                  "type": "divider"
                },
                {
                  "type": "heading",
                  "level": 3,
                  "text": "Selected Files"
                },
                {
                  "type": "list",
                  "data": "{{variables.uploadFiles}}",
                  "itemTemplate": {
                    "type": "file-item",
                    "filename": "{{item.name}}",
                    "size": "{{item.size | fileSize}}",
                    "type": "{{item.type}}",
                    "onRemove": "workflow:removeFile:{{index}}"
                  }
                },
                {
                  "type": "button",
                  "text": "Upload {{variables.uploadFiles.length}} file(s)",
                  "variant": "primary",
                  "icon": "upload",
                  "loading": "{{variables.isUploading}}",
                  "onClick": "workflow:uploadFiles"
                }
              ]
            },
            {
              "type": "conditional",
              "condition": "{{variables.isUploading}}",
              "children": [
                {
                  "type": "progress",
                  "value": "{{variables.uploadProgress}}",
                  "label": "Uploading..."
                }
              ]
            },
            {
              "type": "conditional",
              "condition": "{{variables.uploadResult}}",
              "children": [
                {
                  "type": "alert",
                  "variant": "{{variables.uploadResult.success ? 'success' : 'error'}}",
                  "title": "{{variables.uploadResult.success ? 'Upload Complete' : 'Upload Failed'}}",
                  "message": "{{variables.uploadResult.message}}"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "path": "/documents",
      "name": "Documents",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Documents"
            },
            {
              "type": "container",
              "class": "filters",
              "children": [
                {
                  "type": "select",
                  "name": "typeFilter",
                  "placeholder": "All Types",
                  "options": [
                    { "value": "", "label": "All Types" },
                    { "value": "W-2", "label": "W-2" },
                    { "value": "1099", "label": "1099" },
                    { "value": "Invoice", "label": "Invoice" },
                    { "value": "Receipt", "label": "Receipt" }
                  ],
                  "onChange": "workflow:filterDocuments"
                },
                {
                  "type": "select",
                  "name": "statusFilter",
                  "placeholder": "All Status",
                  "options": [
                    { "value": "", "label": "All Status" },
                    { "value": "pending", "label": "Pending" },
                    { "value": "processing", "label": "Processing" },
                    { "value": "completed", "label": "Completed" },
                    { "value": "failed", "label": "Failed" },
                    { "value": "needs_review", "label": "Needs Review" }
                  ],
                  "onChange": "workflow:filterDocuments"
                }
              ]
            }
          ]
        },
        {
          "type": "card",
          "children": [
            {
              "type": "table",
              "data": "{{collections.UserDocuments}}",
              "columns": [
                {
                  "key": "filename",
                  "label": "Filename",
                  "sortable": true
                },
                {
                  "key": "type",
                  "label": "Type",
                  "render": "badge",
                  "sortable": true
                },
                {
                  "key": "status",
                  "label": "Status",
                  "render": "status-badge",
                  "sortable": true
                },
                {
                  "key": "ocr_confidence",
                  "label": "Confidence",
                  "render": "percentage",
                  "sortable": true
                },
                {
                  "key": "created_at",
                  "label": "Created",
                  "render": "date",
                  "sortable": true
                },
                {
                  "key": "actions",
                  "label": "Actions",
                  "render": "actions",
                  "actions": [
                    {
                      "icon": "eye",
                      "tooltip": "View",
                      "onClick": "navigate:/documents/{{row.id}}"
                    },
                    {
                      "icon": "refresh-cw",
                      "tooltip": "Reprocess",
                      "onClick": "workflow:reprocessDocument:{{row.id}}"
                    },
                    {
                      "icon": "trash",
                      "tooltip": "Delete",
                      "onClick": "workflow:deleteDocument:{{row.id}}",
                      "variant": "danger"
                    }
                  ]
                }
              ],
              "pagination": true,
              "pageSize": 20,
              "emptyText": "No documents found"
            }
          ]
        }
      ]
    },
    {
      "path": "/documents/:id",
      "name": "Document Details",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "breadcrumb",
              "items": [
                { "label": "Documents", "href": "/documents" },
                { "label": "{{collections.DocumentDetails.filename}}" }
              ]
            },
            {
              "type": "container",
              "class": "actions",
              "children": [
                {
                  "type": "button",
                  "text": "Download",
                  "variant": "outline",
                  "icon": "download",
                  "onClick": "workflow:downloadDocument"
                },
                {
                  "type": "button",
                  "text": "Reprocess",
                  "variant": "outline",
                  "icon": "refresh-cw",
                  "onClick": "workflow:reprocessDocument:{{routeParams.id}}"
                },
                {
                  "type": "button",
                  "text": "Delete",
                  "variant": "danger",
                  "icon": "trash",
                  "onClick": "workflow:deleteDocument:{{routeParams.id}}"
                }
              ]
            }
          ]
        },
        {
          "type": "grid",
          "columns": 2,
          "gap": 24,
          "children": [
            {
              "type": "card",
              "title": "Document Preview",
              "children": [
                {
                  "type": "document-preview",
                  "src": "{{collections.DocumentDetails.storage_path}}",
                  "type": "{{collections.DocumentDetails.type}}"
                }
              ]
            },
            {
              "type": "container",
              "children": [
                {
                  "type": "card",
                  "title": "Document Info",
                  "children": [
                    {
                      "type": "info-list",
                      "items": [
                        { "label": "Filename", "value": "{{collections.DocumentDetails.filename}}" },
                        { "label": "Type", "value": "{{collections.DocumentDetails.type}}", "render": "badge" },
                        { "label": "Status", "value": "{{collections.DocumentDetails.status}}", "render": "status-badge" },
                        { "label": "Confidence", "value": "{{(collections.DocumentDetails.ocr_confidence * 100).toFixed(1)}}%" },
                        { "label": "Created", "value": "{{collections.DocumentDetails.created_at | date}}" },
                        { "label": "Updated", "value": "{{collections.DocumentDetails.updated_at | date}}" }
                      ]
                    }
                  ]
                },
                {
                  "type": "card",
                  "title": "Extracted Data",
                  "children": [
                    {
                      "type": "json-viewer",
                      "data": "{{collections.DocumentDetails.extracted_data}}",
                      "expandLevel": 2
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "grid",
          "columns": 2,
          "gap": 24,
          "children": [
            {
              "type": "card",
              "title": "Similar Documents",
              "children": [
                {
                  "type": "list",
                  "data": "{{collections.SimilarDocuments.data.results}}",
                  "itemTemplate": {
                    "type": "similar-document-item",
                    "filename": "{{item.document.filename}}",
                    "similarity": "{{(item.similarity * 100).toFixed(1)}}%",
                    "href": "/documents/{{item.document.id}}"
                  },
                  "emptyText": "No similar documents found"
                }
              ]
            },
            {
              "type": "card",
              "title": "Data Inconsistencies",
              "children": [
                {
                  "type": "conditional",
                  "condition": "{{collections.DocumentInconsistencies.data.inconsistencies.length > 0}}",
                  "children": [
                    {
                      "type": "alert",
                      "variant": "warning",
                      "title": "Inconsistencies Detected",
                      "message": "Review the following data discrepancies"
                    },
                    {
                      "type": "list",
                      "data": "{{collections.DocumentInconsistencies.data.inconsistencies}}",
                      "itemTemplate": {
                        "type": "inconsistency-item",
                        "field": "{{item.field}}",
                        "value": "{{item.value}}",
                        "expectedRange": "{{item.expected_range}}",
                        "severity": "{{item.severity}}"
                      }
                    }
                  ]
                },
                {
                  "type": "conditional",
                  "condition": "{{collections.DocumentInconsistencies.data.inconsistencies.length === 0}}",
                  "children": [
                    {
                      "type": "empty-state",
                      "icon": "check-circle",
                      "title": "No Inconsistencies",
                      "description": "All data values are within expected ranges"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "path": "/search",
      "name": "Search",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Search Documents"
            }
          ]
        },
        {
          "type": "card",
          "children": [
            {
              "type": "container",
              "class": "search-form",
              "children": [
                {
                  "type": "input",
                  "name": "searchQuery",
                  "placeholder": "Search by content, employer name, amounts...",
                  "icon": "search",
                  "class": "flex-1",
                  "onInput": "debounce:workflow:search:300"
                },
                {
                  "type": "select",
                  "name": "documentType",
                  "placeholder": "All Types",
                  "options": [
                    { "value": "", "label": "All Types" },
                    { "value": "W-2", "label": "W-2" },
                    { "value": "1099", "label": "1099" },
                    { "value": "Invoice", "label": "Invoice" }
                  ],
                  "onChange": "workflow:search"
                },
                {
                  "type": "button",
                  "text": "Search",
                  "variant": "primary",
                  "icon": "search",
                  "onClick": "workflow:search"
                }
              ]
            }
          ]
        },
        {
          "type": "conditional",
          "condition": "{{variables.isSearching}}",
          "children": [
            {
              "type": "loading-spinner",
              "text": "Searching..."
            }
          ]
        },
        {
          "type": "conditional",
          "condition": "{{collections.SearchResults.data && !variables.isSearching}}",
          "children": [
            {
              "type": "text",
              "content": "Found {{collections.SearchResults.data.results.length}} results",
              "class": "text-sm text-gray-500 mb-4"
            },
            {
              "type": "grid",
              "columns": 1,
              "gap": 16,
              "children": [
                {
                  "type": "repeat",
                  "data": "{{collections.SearchResults.data.results}}",
                  "template": {
                    "type": "search-result-card",
                    "document": "{{item.document}}",
                    "similarity": "{{item.similarity}}",
                    "content": "{{item.content}}",
                    "highlights": "{{item.highlights}}",
                    "onClick": "navigate:/documents/{{item.document.id}}"
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "conditional",
          "condition": "{{collections.SearchResults.data && collections.SearchResults.data.results.length === 0 && !variables.isSearching}}",
          "children": [
            {
              "type": "empty-state",
              "icon": "search",
              "title": "No Results Found",
              "description": "Try adjusting your search terms or filters"
            }
          ]
        }
      ]
    },
    {
      "path": "/audio",
      "name": "Audio Transcriptions",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Audio Transcriptions"
            },
            {
              "type": "button",
              "text": "Upload Audio",
              "variant": "primary",
              "icon": "mic",
              "href": "/upload"
            }
          ]
        },
        {
          "type": "card",
          "children": [
            {
              "type": "table",
              "data": "{{collections.AudioTranscriptions}}",
              "columns": [
                { "key": "filename", "label": "Filename", "sortable": true },
                { "key": "language", "label": "Language", "render": "badge" },
                { "key": "duration_seconds", "label": "Duration", "render": "duration" },
                { "key": "speaker_count", "label": "Speakers" },
                { "key": "status", "label": "Status", "render": "status-badge" },
                { "key": "created_at", "label": "Created", "render": "date" },
                {
                  "key": "actions",
                  "label": "Actions",
                  "render": "actions",
                  "actions": [
                    { "icon": "eye", "tooltip": "View", "onClick": "workflow:viewTranscription:{{row.id}}" },
                    { "icon": "download", "tooltip": "Download", "onClick": "workflow:downloadTranscription:{{row.id}}" }
                  ]
                }
              ],
              "pagination": true,
              "pageSize": 20,
              "emptyText": "No audio transcriptions yet"
            }
          ]
        }
      ]
    },
    {
      "path": "/text",
      "name": "Text Extractions",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Text Extractions"
            }
          ]
        },
        {
          "type": "card",
          "children": [
            {
              "type": "table",
              "data": "{{collections.TextExtractions}}",
              "columns": [
                { "key": "source", "label": "Source", "sortable": true },
                { "key": "subject", "label": "Subject" },
                { "key": "sentiment", "label": "Sentiment", "render": "sentiment-badge" },
                { "key": "status", "label": "Status", "render": "status-badge" },
                { "key": "created_at", "label": "Created", "render": "date" },
                {
                  "key": "actions",
                  "label": "Actions",
                  "render": "actions",
                  "actions": [
                    { "icon": "eye", "tooltip": "View", "onClick": "workflow:viewExtraction:{{row.id}}" }
                  ]
                }
              ],
              "pagination": true,
              "pageSize": 20,
              "emptyText": "No text extractions yet"
            }
          ]
        }
      ]
    },
    {
      "path": "/profile",
      "name": "Profile",
      "isPublic": false,
      "layout": "app",
      "components": [
        {
          "type": "container",
          "class": "page-header",
          "children": [
            {
              "type": "heading",
              "level": 1,
              "text": "Profile Settings"
            }
          ]
        },
        {
          "type": "grid",
          "columns": 2,
          "gap": 24,
          "children": [
            {
              "type": "card",
              "title": "Personal Information",
              "children": [
                {
                  "type": "form",
                  "id": "profileForm",
                  "children": [
                    {
                      "type": "input",
                      "name": "fullName",
                      "label": "Full Name",
                      "value": "{{collections.CurrentUser.user_metadata.full_name}}"
                    },
                    {
                      "type": "input",
                      "name": "email",
                      "label": "Email",
                      "value": "{{collections.CurrentUser.email}}",
                      "disabled": true
                    },
                    {
                      "type": "button",
                      "text": "Update Profile",
                      "variant": "primary",
                      "onClick": "workflow:updateProfile"
                    }
                  ]
                }
              ]
            },
            {
              "type": "card",
              "title": "Change Password",
              "children": [
                {
                  "type": "form",
                  "id": "passwordForm",
                  "children": [
                    {
                      "type": "input",
                      "name": "currentPassword",
                      "label": "Current Password",
                      "inputType": "password"
                    },
                    {
                      "type": "input",
                      "name": "newPassword",
                      "label": "New Password",
                      "inputType": "password",
                      "minLength": 8
                    },
                    {
                      "type": "input",
                      "name": "confirmNewPassword",
                      "label": "Confirm New Password",
                      "inputType": "password"
                    },
                    {
                      "type": "button",
                      "text": "Change Password",
                      "variant": "primary",
                      "onClick": "workflow:changePassword"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "card",
          "title": "Account Statistics",
          "children": [
            {
              "type": "grid",
              "columns": 4,
              "gap": 16,
              "children": [
                {
                  "type": "stat-card",
                  "title": "Total Documents",
                  "value": "{{collections.UserDocuments.length}}",
                  "icon": "file-text"
                },
                {
                  "type": "stat-card",
                  "title": "Audio Files",
                  "value": "{{collections.AudioTranscriptions.length}}",
                  "icon": "mic"
                },
                {
                  "type": "stat-card",
                  "title": "Text Extractions",
                  "value": "{{collections.TextExtractions.length}}",
                  "icon": "file-text"
                },
                {
                  "type": "stat-card",
                  "title": "Member Since",
                  "value": "{{collections.CurrentUser.created_at | date:MMMM YYYY}}",
                  "icon": "calendar"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "layouts": {
    "auth": {
      "components": [
        {
          "type": "container",
          "class": "auth-layout min-h-screen flex items-center justify-center bg-gray-50",
          "children": [
            { "type": "slot", "name": "content" }
          ]
        }
      ]
    },
    "app": {
      "components": [
        {
          "type": "container",
          "class": "app-layout min-h-screen bg-gray-50",
          "children": [
            {
              "type": "sidebar",
              "logo": "/logo.svg",
              "items": [
                { "label": "Dashboard", "icon": "home", "href": "/dashboard" },
                { "label": "Upload", "icon": "upload", "href": "/upload" },
                { "label": "Documents", "icon": "file-text", "href": "/documents" },
                { "label": "Search", "icon": "search", "href": "/search" },
                { "label": "Audio", "icon": "mic", "href": "/audio" },
                { "label": "Text", "icon": "file", "href": "/text" }
              ],
              "footer": [
                { "label": "Profile", "icon": "user", "href": "/profile" },
                { "label": "Logout", "icon": "log-out", "onClick": "workflow:logout" }
              ]
            },
            {
              "type": "container",
              "class": "main-content ml-64 p-8",
              "children": [
                {
                  "type": "header",
                  "class": "flex justify-between items-center mb-8",
                  "children": [
                    { "type": "slot", "name": "header" },
                    {
                      "type": "notification-bell",
                      "count": "{{collections.UnreadNotificationsCount.count}}",
                      "onClick": "workflow:toggleNotifications"
                    }
                  ]
                },
                { "type": "slot", "name": "content" }
              ]
            }
          ]
        }
      ]
    }
  }
}
