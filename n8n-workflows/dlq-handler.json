    {
      "parameters": {
        "jsCode": "// Process retry logic\nconst items = $input.all();\nconst prevItems = $('Split Items').all();\nconst results = [];\n\nfor (let i = 0; i < items.length; i++) {\n  const dlqItem = prevItems[i].json;\n  const maxRetries = dlqItem.max_retries || 5;\n  const currentRetry = dlqItem.retry_attempt || 1;\n  \n  // Simulate retry result (in production, this would call the actual processing endpoint)\n  // For demo: 70% success rate\n  const success = Math.random() > 0.3;\n  \n  let status, nextRetryAt;\n  \n  if (success) {\n    status = 'completed';\n    nextRetryAt = null;\n  } else if (currentRetry >= maxRetries) {\n    status = 'abandoned';\n    nextRetryAt = null;\n  } else {\n    status = 'pending';\n    // Exponential backoff: 2^retry minutes\n    const delayMinutes = Math.pow(2, currentRetry);\n    nextRetryAt = new Date(Date.now() + delayMinutes * 60 * 1000).toISOString();\n  }\n  \n  results.push({\n    json: {\n      dlq_id: dlqItem.id,\n      status,\n      next_retry_at: nextRetryAt,\n      success,\n      retry_count: currentRetry,\n      resource_type: dlqItem.resource_type\n    }\n  });\n}\n\nreturn results;"
      },
