    {
      "parameters": {
        "jsCode": "// Parse incoming request and create task\nconst items = $input.all();\nconst results = [];\n\nfor (const item of items) {\n  const body = item.json.body || item.json;\n  \n  // Determine task type and priority\n  let taskType = body.task_type || 'document';\n  let priority = body.priority || 2;\n  \n  // Auto-prioritize based on criteria\n  if (body.urgent === true) {\n    priority = 1;\n  }\n  \n  // Map task type to worker workflow\n  const workerMapping = {\n    'document': 'document-worker',\n    'audio': 'audio-worker',\n    'text': 'text-worker',\n    'onboarding': 'onboarding-worker',\n    'communication': 'communication-worker'\n  };\n  \n  const workflowName = workerMapping[taskType] || 'document-worker';\n  \n  results.push({\n    json: {\n      workflow_name: workflowName,\n      task_type: taskType,\n      priority: priority,\n      payload: body.payload || body,\n      status: 'pending'\n    }\n  });\n}\n\nreturn results;"
      },
